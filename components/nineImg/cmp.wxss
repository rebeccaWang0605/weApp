/* components/nineImg/cmp.wxss */
.container{
    width:700rpx;
}
.image-container{
    display:flex;
    flex-wrap:wrap;
    justify-content: space-between;
    width:700rpx;
    height:700rpx;
    overflow: hidden;
    position: relative;
}
.image{
    width:31%;
    height:31%;
}
.text-container{
    padding:0 30rpx;
    font-size:40rpx;
    font-weight:400;
    letter-spacing: 2rpx;
}
.image-num{
    position:absolute;
    bottom:80rpx;
    right:80rpx;
    color:#fff;
    font-size:24rpx;
    background-color:rgba(0,0,0,0.7);
    padding:5rpx 10rpx;
}
